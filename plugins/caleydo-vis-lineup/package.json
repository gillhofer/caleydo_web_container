{
  "name" : "caleydo-vis-lineup",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.5.5",
    "lineupjs" : "git@github.com:Caleydo/lineup.js.git"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "id": "caleydo-vis-lineup",
        "name": "LineUp",
        "sizeDependsOnDataDimension": [
          false,
          true
        ],
        "filter": "table"
      }
    ],
    "requirejs-config": {
      "shim": {
        "lineupjs": ["css!${baseUrl}/lineupjs/css/style"]
      }
    }
  }
}
