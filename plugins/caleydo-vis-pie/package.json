{
  "name" : "caleydo-vis-pie",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.4.11"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "id": "caleydo-vis-pie",
        "name": "Pie",
        "icon" : "icon.png",
        "size": {
          "size" : [100, 100],
          "scale": "aspect"
        },
        "filter": "eval!function (data) { return data.desc.type === 'vector' && (data.desc.value.type === 'categorical'); }"
      }
    ]
  }
}
