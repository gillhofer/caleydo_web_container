{
  "name" : "caleydo",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.5.5",
    "jquery": "~1.10.2",
    "requirejs": "~2.1.17",
    "require-css": "~0.1.8"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "datatype",
        "id": "matrix",
        "module": "caleydo/matrix"
      },
      {
        "type": "datatype",
        "id": "table",
        "module": "caleydo/table"
      },
      {
        "type": "datatype",
        "id": "vector",
        "module": "caleydo/vector"
      },
      {
        "type": "datatype",
        "id": "stratification",
        "module": "caleydo/stratification"
      }
    ],
    "requirejs-config": {
      "map": {
        "*": {
          "css": "${baseUrl}/require-css/css.js"
        }
      }
    },
    "requirejs-bower": {
      "ignore" : ["requirejs", "require-css"]
    }
  }
}
