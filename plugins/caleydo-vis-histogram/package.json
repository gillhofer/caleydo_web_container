{
  "name" : "caleydo-vis-histogram",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.4.11"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "id": "caleydo-vis-histogram",
        "name": "Histogram",
        "icon": "icon.png",
        "filter": [ "(vector|matrix)", "(categorical|real|int)" ]
      },
      {
        "type": "vis",
        "id": "caleydo-vis-mosaic",
        "folder": "caleydo-vis-histogram",
        "name": "Mosaic",
        "factory": "createMosaic",
        "icon": "icon_mosaic.png",
        "sizeDependsOnDataDimension": [
          false,
          true
        ],
        "scaling" : "height-only",
        "filter": [ "vector", "categorical" ]
      }
    ]
  }
}
