{
  "name" : "lineup",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.4.11",
    "lineupjs" : "git@github.com:Caleydo/lineup.js.git#singlesvg"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "name": "lineup",
        "size": "eval!function (dim) { return [Math.min(dim[1] * 100, 1000), Math.min(dim[0] * 20, 600)]; }",
        "filter": "table"
      }
    ],
    "requirejs-config": {
      "shim": {
        "lineupjs": ["css!${basedir}/lineupjs/css/style"]
      }
    }
  }
}