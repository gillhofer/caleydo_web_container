{
  "name" : "caleydo-vis-heatmap",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.4.11"
  },
  "caleydo" : {
    "plugins": [
      {
        "type": "vis",
        "id": "caleydo-vis-heatmap",
        "name": "HeatMap",
        "icon": "icon.svg",
        "size": "eval!function (dim) { return [dim[1] * 10, dim[0] * 10]; }",
        "filter": "matrix"
      }
    ]
  }
}